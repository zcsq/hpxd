function goumai(event) {

	if(isbd==0){
		ljbd();
		return false;
	}
    var clickedElement = event.target.closest('.list');

    var imgSrc = clickedElement.querySelector('.xximg').src;
    var itemName = clickedElement.querySelector('#mingzi').innerText;
    var itemPrice = clickedElement.querySelector('#jiage').innerText;

    document.querySelector('#spimg').src=imgSrc;
    document.querySelector('#spmz').innerText=itemName;
    document.querySelector('#spmz2').innerText=itemName;
    document.querySelector('#spjg').innerText=itemPrice;
    document.querySelector('#sydq').innerText=sydq;
	ddform.style.display="block";


}

var spform=document.getElementById("spform");
var sp=document.getElementById("ydvideo");


function tjgm(){
	
	itemPrice=document.querySelector('#spjg').innerText;
	csjg=sydq - itemPrice;
	if(csjg<0){
		alert('余额不足！');
	}else{
	sydq=sydq - itemPrice;
	document.querySelector('#ye').innerText=sydq;
	
	mask.style.display = 'block';
  		setTimeout(function() {
		ddform.style.display="none";
    	mask.style.display = 'none';
		spform.style.display="block"
		sp.play();
  }, 2000);

	}

}







