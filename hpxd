	<script type="text/javascript">
		function showWechatCustomer() {
		      var wechatCustomer = document.getElementById("wechat-customer");
		      wechatCustomer.style.display = "block";
		    }
		
		    function hideWechatCustomer() {
		      var wechatCustomer = document.getElementById("wechat-customer");
		      wechatCustomer.style.display = "none";
		    }
		
		
		
		
		// 声明一个全局数组，用于存储收藏的物品ID
		var collectedItems = [];
		
		function gz(obj, dataid) {
			event.stopPropagation();
		    // 检查物品是否已经收藏
		    var isCollected = collectedItems.includes(dataid);
		
		    if (isCollected) {
		        // 如果已经收藏，从全局数组中移除，并更改背景图
		        obj.style.backgroundImage = "url('images/g1.png')";
		        var index = collectedItems.indexOf(dataid);
		        if (index !== -1) {
		            collectedItems.splice(index, 1);
		        }
		    } else {
		        // 如果未收藏，将其添加到全局数组中，并更改背景图
		        obj.style.backgroundImage = "url('images/g2.png')";
		        collectedItems.push(dataid);
		    }
		    console.log(collectedItems);
		}
		
		
		
		function ygz() {

      var elements = document.querySelectorAll('.nav1 div');
            elements.forEach(function(element) {
                element.classList.remove('act');
            });

            // Add 'act' class to the clicked element
            event.currentTarget.classList.add('act');

		    // 获取所有具有data-id属性的元素
		    var elementsWithDataId = document.querySelectorAll('[data-id]');
		
		    // 遍历这些元素
		    elementsWithDataId.forEach(function (element) {
		        // 获取当前元素的data-id值
		        var dataId = element.getAttribute('data-id');
		
		        // 检查data-id值是否存在于collectedItems数组中
		        var isCollected = collectedItems.includes(dataId);
		
		        // 如果存在，则给该元素添加guanzhu类
		        if (isCollected) {
		            element.classList.add('guanzhu');
		        } else {
		            // 如果不存在，则确保移除guanzhu类
		            element.classList.remove('guanzhu');
		        }
		    });
		
		  // 获取所有具有 "list" 类的元素
		  var listElements = document.querySelectorAll('.list');
		
		  // 获取所有具有 "guanzhu" 类的元素
		  var guanzhuElements = document.querySelectorAll('.guanzhu');
		
		  // 遍历并设置 display:none
		  listElements.forEach(function(element) {
		    element.style.display = 'none';
		  });
		
		  // 遍历并设置 display:block
		  guanzhuElements.forEach(function(element) {
		    element.style.display = 'block';
		  });
		}
		
		
		function qbsp(){

      var elements = document.querySelectorAll('.nav1 div');
            elements.forEach(function(element) {
                element.classList.remove('act');
            });

      
            event.currentTarget.classList.add('act');

		  // 获取所有具有 "list" 类的元素
		  var listElements = document.querySelectorAll('.list');
		      listElements.forEach(function(element) {
		    element.style.display = 'block';
		  });
		
		}
		
		
		
		
		
		
		// 获取所有倒计时元素
		var countdowns = document.querySelectorAll('.time');
		
		// 定义周期时长（单位：毫秒）
		var period = 2 * 24 * 60 * 60 * 1000;
		
		// 定义倒计时函数
		function updateCountdown() {
		  // 遍历所有倒计时元素
		  for (var i = 0; i < countdowns.length; i++) {
		    // 获取当前时间和周期结束时间
		    var now = new Date();
		    var end = new Date(Math.ceil(now.getTime() / period) * period);
		
		    // 计算剩余时间
		    var diff = end.getTime() - now.getTime();
		    var days = Math.floor(diff / (24 * 60 * 60 * 1000));
		    var hours = Math.floor((diff % (24 * 60 * 60 * 1000)) / (60 * 60 * 1000));
		    var minutes = Math.floor((diff % (60 * 60 * 1000)) / (60 * 1000));
		    var seconds = Math.floor((diff % (60 * 1000)) / 1000);
		
		    // 更新倒计时元素
		    countdowns[i].innerHTML = '<span>' + days + '</span>天' +                       '<span>' + hours + '</span>时' +                       '<span>' + minutes + '</span>分' +                       '<span>' + seconds + '</span>秒后 结束抢购';
		  }
		
		  // 每秒更新一次倒计时
		  setTimeout(updateCountdown, 1000);
		}
		
		// 启动倒计时
		updateCountdown();
		
		
		
		
		
		
		
		
		
		function hz(obj) {
		    var listElement = document.getElementById('list');
		
		    // 切换类
		    if (listElement.classList.contains('lists2')) {
		      listElement.classList.remove('lists2');
		      obj.style.backgroundImage = "url('images/z.png')";
		    } else {
		      obj.style.backgroundImage = "url('images/h.png')";
		      listElement.classList.add('lists2');
		    }
		  }
		
		
