var zsformxx = document.querySelector('#zsform');
var ddform = document.querySelector('#tjdd');
var mask = document.querySelector('.mask')

var isbd=0;
function tj(){

 
  	mask.style.display = 'block';
  		setTimeout(function() {
    	mask.style.display = 'none';
		bdform.style.display="none";

  // 获取表单元素  
  var gameIdInput = document.getElementById("form").elements["id"];  
  var gameDqSelect = document.getElementById("form").elements["dq"];  
  
  // 获取游戏ID和游戏大区  
  var gameId = gameIdInput.value;  
  var gameDq = gameDqSelect.value;  
  
var jsxx= document.querySelector('.jsxx');
var ljbd= document.querySelector('.ljbd');

jsxx.innerHTML="角色ID："+gameId;
ljbd.innerHTML="所在大区："+gameDq;

isbd=1;
 document.querySelector('#gmjs').innerHTML=gameId;



  }, 2000);
}

    function ljbd() {

		bdform.style.display="block"

}

var sydq=9999;

